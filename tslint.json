{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "interface-name": [
            true,
            "never-prefix"
        ],
        "trailing-comma": [
            true,
            {
                "multiline":"never",
                "singleline":"never"
            }
        ],
        "object-literal-sort-keys": [
            false
        ],
        "typedef": [
            true,
            "call-signature",
            "parameter"
        ]
    },
    "rulesDirectory": []
}